                            <hr>
                            <div>
                                <span class="h2"><abbr title="1356 calories">@Model.maintenanceHigherCarbs</abbr></span>
                                <br>
                                <span class="cals">karbonhidrat</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- end row -->

